{
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/.*",
      "headers": {
        "WWW-Authenticate": "Basic realm=\"Zotas Preview\""
      },
      "dest": "/api/auth"
    }
  ]


}
